generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id             String    @id @default(cuid())
  first_name     String
  last_name      String
  student_number BigInt    @unique
  cohort     String
  pass_id        String @unique
}

model inventory {
  id          String    @id @default(cuid())
  name        String
  description String
}
