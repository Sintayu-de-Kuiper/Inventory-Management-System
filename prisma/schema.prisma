// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id
  first_name String
  last_name String
  student_number Int
  class_name String
  pass_id Int
  admin String @default("false")
}

model inventory {
  id    Int    @id
  name String
  description String
}


