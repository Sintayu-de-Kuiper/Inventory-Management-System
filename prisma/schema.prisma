// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  user_id    Int    @id
  first_name String
  last_email String @unique
  password   String
  student_number Int
  class_name String
  unique_id Int
}
